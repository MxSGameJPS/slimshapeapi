{
  "version": 2,
  "builds": [{ "src": "api/index.js", "use": "@vercel/node" }],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.js" },
  { "src": "/swagger", "dest": "/public/swagger.html" },
  { "src": "/swagger.html", "dest": "/public/swagger.html" },
  { "src": "/swagger.json", "dest": "/public/swagger.json" },
  { "src": "/swagger.css", "dest": "/public/swagger.css" }
  ]
}
